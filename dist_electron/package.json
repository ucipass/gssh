    "xterm-addon-fit": "^0.1.0",
    "xterm-addon-web-links": "^0.1.0"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.4.4",
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "babel-eslint": "^10.0.1",
    "bootstrap": "^4.3.1",
    "electron": "^9.1.0",
    "eslint": "^5.16.0",
